{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["lib/"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": false
    },
    "templates": {
        "systemName": "Voxel51 Vision Services API",
        "footer": "Voxel51, LLC",
        "copyright": 2018,
        "theme": "slate",
        "sort": false,
        "search": true,
        "outputSourceFiles": false
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "./docs",
        "recurse": true
    }
}
