{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["lib/", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "Vision Services API",
        "disableSort": true
    },
    "opts": {
        "template": "node_modules/jsdoc-template",
        "encoding": "utf8",
        "recurse": true,
        "destination": "docs/build"
    }
}
