{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["lib/", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "referenceTitle": "Voxel51 Platform API JavaScript Client Library",
        "disableSort": true
    },
    "opts": {
        "template": "node_modules/jsdoc-template",
        "encoding": "utf8",
        "recurse": true
    }
}
